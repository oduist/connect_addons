<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-inherit="connect.active_calls_popup" t-inherit-mode="extension">
        <xpath expr="//th[hasclass('partner-th')]" position="after">
            <th>Ticket</th>
        </xpath>
        <xpath expr="//td[hasclass('partner-td')]" position="after">
            <td t-on-click="(ev) => this._onClickTicket(ev, call.ticket)" t-attf-title="{{call.ticket ? call.ticket[1] : ''}}"><t t-if="call.ticket" t-out="call.ticket[1]"/></td>
        </xpath>
    </t>

</templates>