<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_connect_transfer_wizard_form" model="ir.ui.view">
        <field name="name">view_connect_transfer_wizard_form</field>
        <field name="model">connect.transfer_wizard</field>
        <field name="arch" type="xml">
            <form string="Phone Wizard">
                <group>
                    <field name="phone_number"/>
                </group>
                <footer>
                    <button string="Confirm" type="object" name="action_confirm" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>
